{
    "eslint.options": {
        "envs": ["browser", "es6", "node"],
        "globals": [
            "JSON",
            "$",
            "ko",
            "define",
            "require",
            "requirejs",
            "CORE",
            "Logger",
            "expect",
            "it",
            "beforeEach",
            "afterEach",
            "describe",
            "afterAll",
            "oj"
        ],
        "rules": {
            "no-multiple-empty-lines": ["error", {
                "max": 1,
                "maxEOF": 1
            }],
            "comma-dangle": [2, "never"],
            "no-cond-assign": [2, "always"],
            "no-constant-condition": 1,
            "no-dupe-args": 2,
            "no-void": 2,
            "prefer-destructuring": 1,
            "prefer-rest-params": 1,
            "prefer-const": 2,
            "require-yield": 2,
            "prefer-spread": 1,
            "prefer-template": 1,
            "no-useless-return": 2,
            "no-useless-escape": 2,
            "no-dupe-keys": 2,
            "no-duplicate-case": 2,
            "no-useless-concat": 2,
            "no-empty-character-class": 2,
            "no-empty": 2,
            "no-compare-neg-zero": 2,
            "no-sparse-arrays": 2,
            "no-ex-assign": 2,
            "no-empty-pattern": 2,
            "no-extra-semi": 2,
            "no-else-return": 2,
            "no-func-assign": 2,
            "no-new-func": 2,
            "no-template-curly-in-string": 1,
            "no-invalid-regexp": 2,
            "constructor-super": 2,
            "no-div-regex": 2,
            "no-proto": 2,
            "require-await": 2,
            "no-const-assign": 2,
            "no-duplicate-imports": 2,
            "no-useless-rename": 2,
            "prefer-arrow-callback": 1,
            "no-new-symbol": 2,
            "no-useless-constructor": 2,
            "no-useless-computed-key": 2,
            "no-this-before-super": 2,
            "radix": 2,
            "no-control-regex": 2,
            "no-irregular-whitespace": 2,
            "no-unsafe-negation": 2,
            "no-unsafe-finally": 2,
            "no-regex-spaces": 2,
            "no-obj-calls": 2,
            "no-unexpected-multiline": 2,
            "no-unreachable": 2,
            "no-return-await": 2,
            "no-inner-declarations": 2,
            "valid-typeof": 2,
            "no-useless-call": 2,
            "strict": [2, "safe"],
            "arrow-body-style": [2, "as-needed"],
            "arrow-parens": [2, "as-needed"],
            "default-case": 2,
            "curly": 2,
            "no-self-assign": 2,
            "eqeqeq": 2,
            "use-isnan": 2,
            "no-alert": 2,
            "no-console": 2,
            "no-throw-literal": 2,
            "no-case-declarations": 2,
            "no-labels": 2,
            "no-eq-null": 2,
            "no-eval": 2,
            "no-implied-eval": 2,
            "no-loop-func": 2,
            "no-debugger": 2,
            "no-sequences": 2,
            "no-lone-blocks": 2,
            "no-native-reassign": 2,
            "no-new-wrappers": 2,
            "no-return-assign": 2,
            "no-self-compare": 2,
            "no-with": 2,
            "camelcase": 2,
            "quotes": [2, "double"],
            "semi": [2, "always"],
            "no-unused-vars": 2,
            "no-undefined": 2,
            "no-undef-init": 2,
            "no-var": 1,
            "template-curly-spacing": ["error", "never"],
            "no-undef": 2,
            "no-use-before-define": 2
        }
    },
	"html-css-class-completion.excludeGlobPattern": "/{.history,node_modules}/*",
	"html-css-class-completion.includeGlobPattern": "**/*.{scss,css,html}"
}